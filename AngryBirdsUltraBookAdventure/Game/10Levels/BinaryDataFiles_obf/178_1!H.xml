<UIDescription>
	<Views>
		<PopupView_EasterHunt name="EasterHunt" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_EasterHunt>

		<PopupView_Syncing name="Syncing" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Syncing>

		<PopupView_EggFound name="EggFound" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER" scaleH="True" scaleV="True" aspectRatioFixed="TRUE">
		</PopupView_EggFound>

		<PopupServerUpdated_Error name="ServerUpdatedBox" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupServerUpdated_Error>

		<PopupView_Error name="Errorbox" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Error>

		<PopupView_Warning name="Warningbox" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Warning>

		<PopupView_Embed name="EmbedLevel" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Embed>

        <View_CutScene name="ViewCutScene" width="940" height="570">
            <Button name="Button_Skip" visible="False" MouseUp="Skip" alignH="LEFT" alignV="BOTTOM" scaleOnMouseOver="True"/>

            <MovieClip name="MovieClip_Loading" visible="true" alignH="CENTER" alignV="CENTER" />

            <Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON" alignH="RIGHT" alignV="BOTTOM"/>
        </View_CutScene>

        <View_LevelSelection name="ViewLevelSelection" width="940" height="570">
           <MovieClip name="MovieClip_BackGround" scaleH="True" scaleV="True"/>



            <Container name="Container_LevelSelection" alignH="CENTER" alignV="CENTER" scaleH="DOWN" scaleV="DOWN" aspectRatioFixed="TRUE" width="1080" height="725">
                <Button name="Button_Prev" MouseUp="Prev" scaleOnMouseOver="TRUE"/>
                <Button name="Button_Next" MouseUp="Next" scaleOnMouseOver="TRUE"/>
                <TextField name="TextField_LevelNumberSmall" text="1" enabled="True" />

               <!-- <TextField name="TextField_LevelName" enabled="False" /> -->
                <TextField name="Textfield_CollectedStars" enabled="False" />

                <Container name="Container_LevelRepeaters" />
            </Container>
            <MovieClip name="MovieClip_LeftCorner" visible="True" enabled="True" alignH="LEFT" alignV="BOTTOM" scaleH="DOWN" scaleV="DOWN" aspectRatioFixed="TRUE"/>
            <MovieClip name="MovieClip_RightCorner" visible="True" enabled="True" alignH="RIGHT" alignV="BOTTOM" scaleH="DOWN" scaleV="DOWN" aspectRatioFixed="TRUE"/>

            <Container name="Container_FriendsButton" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
                <Button name="Button_Friends" MouseUp="FRIENDS_BUTTON" scaleOnMouseOver="TRUE" />
            </Container>

            <Container name="Container_BackButton" scaleH="false" scaleV="false" aspectRatioFixed="TRUE" alignH="LEFT" alignV="BOTTOM">
                <Button name="Button_Back" MouseUp="Back"  scaleOnMouseOver="TRUE"/>
            </Container>

            <Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
        </View_LevelSelection>

        <View_ChapterFacebookSelection name="ViewFacebookChapterSelection" width="940" height="570">
            <MovieClip name="MovieClip_BackGround" scaleH="True" scaleV="True"/>

            <Container name="Container_NewChapterSelection" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
                <MovieClip name="MovieClip_Logo" />
                <Button name="MovieClip_StartPlaying" MouseUp="Chapter0" MouseOver="Chapter0Over" MouseOut="Chapter0Out" />
            </Container>

            <MovieClip name="MovieClip_LeftCorner" visible="True" enabled="True" alignH="LEFT" alignV="BOTTOM" scaleH="DOWN" scaleV="DOWN" aspectRatioFixed="TRUE"/>
            <MovieClip name="MovieClip_RightCorner" visible="True" enabled="True" alignH="RIGHT" alignV="BOTTOM" scaleH="DOWN" scaleV="DOWN" aspectRatioFixed="TRUE"/>

        </View_ChapterFacebookSelection>

		<View_FacebookLevelPause name="ViewFacebookLevelPause" width="940" height="570">
			<MovieClip name="MovieClip_DarkBG" scaleH="True" scaleV="True" visible="true"/>

			<Container name="Container_PauseMenu">
				<MovieClip name="MovieClip_PauseMenuBG" scaleV="True"/>
				<TextField name="TextField_LevelName" text="22-22" enabled="True" />
				<TextField name="TextField_ChapterName" enabled="True" />
				<Button name="Button_Resume" MouseUp="RESUME_LEVEL" alignV="CENTER" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Replay" MouseUp="RESTART_LEVEL" alignV="CENTER" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Menu" MouseUp="MENU" alignV="CENTER" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Help" MouseUp="HELP" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Sound" MouseUp="TOGGLE_SOUNDS" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
			</Container>

            <Container name="Container_advertisement" scaleH="True" scaleV="True" aspectRatioFixed="TRUE" alignH="CENTER" alignV="CENTER">
                <Button name="Button_Learn" MouseUp="LEARN_MORE"/>
            </Container>

			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_FacebookLevelPause>

		<View_FacebookLevelPlay name="ViewFacebookLevelPlay" width="940" height="570">
				<Container name="Container_HiscoreContainer" enabled="False" alignH="RIGHT" visible="False">
						<TextField name="TextField_Hiscore" text="0" enabled="False"/>
						<TextField name="TextField_CurrentScore" text="0" enabled="False"/>
				</Container>

				<Container name="Container_Vs" enabled="False" alignH="RIGHT" width="1024" height="768" scaleH="UP" scaleV="UP" aspectRatioFixed="TRUE" visible="False">
					<MovieClip name="MovieClip_Boom" visible="False"/>
					<MovieClip name="MovieClip_VsAnimation" visible="False"/>


					<Container name="Container_VsHero">
						<MovieClip name="MovieClip_VsProfilePicHero"/>
						<MovieClip name="MovieClip_CrownHero" visible="False"/>
						<TextField name="TextField_VsScore" text="0" enabled="False"/>
						<TextField name="TextField_VsPositionHero" text="0" enabled="False"/>
						<TextField name="TextField_NameHero" text="" enabled="False" visible="true"/>
						<MovieClip name="PlayerNameMask"/>
					</Container>

					<Container name="Container_VsEnemy">
						<MovieClip name="MovieClip_VsProfilePicEnemy"/>
						<MovieClip name="MovieClip_CrownEnemy" visible="False"/>
						<TextField name="TextField_VsScoreEnemy" text="0" enabled="False"/>
						<TextField name="TextField_VsPositionEnemy" text="0" enabled="False"/>
						<TextField name="TextField_NameEnemy" text="" enabled="False" visible="true"/>
						<MovieClip name="EnemyNameMask"/>
					</Container>
				</Container>

				<!-- <Button name="MovieClip_FeedBar" MouseUp="OPEN_FEED_DIALOG" enabled="True" /> -->

				<Container name="Container_ZoomButtons" enabled="False" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE" alignV="BOTTOM" alignH="LEFT">
					<Button name="Button_ZoomIn" MouseUp="ZOOM_IN"/>
					<Button name="Button_ZoomOut" MouseUp="ZOOM_OUT"/>
					<Button name="Button_Magnify"/>
				</Container>

				<Container name="Container_Buttons" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
					<Button name="Button_Pause" MouseUp="PAUSE" alignH="LEFT"/>
					<!-- <Button name="Button_Restart" MouseUp="RESTART_LEVEL" alignH="LEFT" /> -->
				</Container>

				<MovieClip name="IntelWatermark" alignH="RIGHT" alignV="BOTTOM" />

				<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_FacebookLevelPlay>

		<!--  NEW LEVEL END VIEW    -->
		<View_LevelEnd name="ViewLevelEnd" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="940" height="570">
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER">
				<MovieClip name="MovieClip_LevelEndStripeBG" scaleV="True" alignV="CENTER"/>

				<Button name="Button_Menu"  MouseUp="MENU" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" scaleOnMouseOver="TRUE"/>
                <Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="false" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Share_Default"  MouseUp="SHARE_DEFAULT" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Share_Crown"  MouseUp="SHARE_CROWN" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Share_Stars"  MouseUp="SHARE_STARS" scaleOnMouseOver="TRUE" visible="False"/>

                <Button name="Button_Embed"  MouseUp="EMBED" scaleOnMouseOver="TRUE" visible="True"/>

				<MovieClip name="MovieClip_StarLeft" />
				<MovieClip name="MovieClip_StarCenter" />
				<MovieClip name="MovieClip_StarRight" />


				<TextField name="TextField_LevelEndScore" enabled="True" />
 				<TextField name="TextField_LevelEndScoreEffects" enabled="True" />

				<TextField name="TextField_NewHighScore" enabled="True" visible="false" />

				<TextField name="TextField_BestScore" enabled="True" visible="false" />
				<MovieClip name="MovieClip_BestScoreStars" enabled="True" visible="false" />

				<MovieClip name="MovieClip_NewHighScoreBadge" visible="false"/>


			</Container>

            <Button name="Button_Friends" MouseUp="FRIENDS_BUTTON" scaleOnMouseOver="TRUE"/>

			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_LevelEnd>
		<!--  NEW LEVEL END VIEW    -->

		<View_LevelEndFailRio name="ViewLevelEndFail" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="940" height="570">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER">
				<MovieClip name="MovieClip_LevelEndStripeFailBG" scaleV="True" alignV="CENTER"/>/>

				<Button name="Button_Menu"  MouseUp="MENU" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" scaleOnMouseOver="TRUE"/>
				<!-- <Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="false" scaleOnMouseOver="TRUE"/> -->
				<!-- <Button name="Button_MightyEagle"  MouseUp="SHOP" visible="false" scaleOnMouseOver="TRUE"/> -->

				<Button name="Button_Share_Default"  MouseUp="SHARE_DEFAULT" scaleOnMouseOver="TRUE" visible="True"/>
				<Button name="Button_Embed"  MouseUp="EMBED" scaleOnMouseOver="TRUE" visible="True"/>

				<MovieClip name="MovieClip_LevelEndFailedPig" visible="true"/>


			</Container>

            <Button name="Button_Friends" MouseUp="FRIENDS_BUTTON" scaleOnMouseOver="TRUE"/>
			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>

		</View_LevelEndFailRio>

		<View_Credits name="ViewCredits" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="940" height="570">

			<MovieClip name="MovieClip_Loading" visible="true" alignH="CENTER" alignV="CENTER" />

			<Container name="Container_Credits" alignH="CENTER" alignV="CENTER" visible = "False">
				<TextField name="TextField_Version_Number" text="1.0.0 placeholder" enabled="True" />
				<TextField name="TextField_Version_Number_Server" text="1.0.0 placeholder" enabled="True" />

				<Container name="Container_Part4">
					<Button name="App_Engine" MouseUp="APP_ENGINE_BUTTON" />
				</Container>
			</Container>

			<Button name="Button_Back" MouseUp="CREDITS_CLOSE_BUTTON" alignH="LEFT" alignV="BOTTOM" aspectRatioFixed="TRUE" scaleOnMouseOver="TRUE"/>

			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_Credits>

        <!--  VIEW LEVEL LOAD    -->
        <View_LevelLoad name="ViewLevelLoad" width="940" height="570">
            <TextField name="TextField_LevelLoading" alignH="CENTER" alignV="CENTER" enabled="False"/>
        </View_LevelLoad>


	</Views>
	<Components>

	</Components>
</UIDescription>