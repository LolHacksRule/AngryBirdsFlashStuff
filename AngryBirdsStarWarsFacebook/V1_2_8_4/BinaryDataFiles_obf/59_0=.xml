<UIDescription>
        <Popups>
        	<!-- GENERIC(VIEWPORT) BLAST DOORS -->
            <Popup_BlastDoors name="Popup_BlastDoors" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
               <MovieClip name="MovieClip_TileMask" width="760" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>

               <Container name="MovieClip_BlastDoors" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="UP" />
            </Popup_BlastDoors>

			<!-- LEVELEND BLASTDOORS -->
            <Popup_BlastShop name="Popup_BlastShop" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
               <MovieClip name="MovieClip_TileMask" width="760" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>

               <Container name="Container_ShopDoors" width="760" height="570" scaleH="true" scaleV="true">
                   <Container name="Container_LeftDoor" autoAlign="false" visible="true">
                         <Button name="Button_Add" MouseUp="BUY_LEFTADD" />
                   </Container>
                   <Container name="Container_RightDoor" autoAlign="false" visible="true">
                        <Button name="Button_Add" MouseUp="BUY_RIGHTADD" />
                   </Container>
               </Container>
            </Popup_BlastShop>

             <!-- SERVER UPDATE -->
            <Popup_ServerUpdate name="Popup_ServerUpdate" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Reload" MouseUp="RELOAD" scaleOnMouseOver="TRUE" />
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_ServerUpdate>

             <!-- FRIENDS PROMO -->
            <Popup_FriendsPromo name="Popup_FriendsPromo" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_PlayFriends" MouseUp="NAVIGATE_OUT" scaleOnMouseOver="TRUE" />
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE"/>
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_FriendsPromo>

            <!-- ENERGY OUT -->
            <Popup_EnergyOut name="Popup_EnergyOut" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">                
                	<Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE"/>

                    <Button name="Button_AskFriend" MouseUp="ASK" scaleOnMouseOver="TRUE" />

                    <Button name="Button_Shop" MouseUp="SHOP" scaleOnMouseOver="TRUE" />

                    <Button name="Button_CheckInbox" MouseUp="INBOX" scaleOnMouseOver="TRUE" enabled="FALSE" />
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_EnergyOut>

            <!-- EXCLUSIVE SALES -->
            <Popup_ExclusiveSales name="Popup_ExclusiveSales" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <Container name="Container_Content" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
                    <Button name="Button_Buy" MouseUp="BUY" />
                </Container>

                 <MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_ExclusiveSales>

            
            <!-- TOURNAMENT INFO -->
            <Popup_TournamentInfo name="Popup_TournamentInfo" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                
            	<MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                
                <Container name="Container_Content" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE"/>
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_TournamentInfo>

            <!-- SYNC -->        	
            <Popup_Sync name="Popup_Sync" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                     <Container name="Container_Content">
                        <Textfield name="TextField_Detail" />
                        <Textfield name="TextField_Info" />
                     </Container>
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_Sync>
            
            
            <!-- ERROR -->
            <Popup_Error name="Popup_Error" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
            	<MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
            	<MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            	<Container name="Container_Content" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
            </Popup_Error>
            
            
            <!-- ENERGY USED -->
            <Popup_EnergyUsed name="Popup_EnergyUsed" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
            	 <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
            	 
            	 </Container>
            </Popup_EnergyUsed>

            <!-- TOURNAMENT UNLOCKED-->
            <Popup_TournamentUnlocked name="Popup_TournamentUnlocked" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
            	 <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE"/>

                    <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                    <Container name="Container_Content">
                         <Button name="Button_Play" MouseUp="PLAY" scaleOnMouseOver="TRUE" />
                    </Container>
            	 </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_TournamentUnlocked>

			<!-- STARCOIN -->
            <Popup_StarCoin name="Popup_StarCoin" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">

                <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                <MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <Container name="Container_Content" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />

                    <MovieClip name="MovieClip_Loading" />

                    <MovieClip name="MovieClip_Title" />

                    <Button name="Button_Buy0" MouseUp="BUY0" />
                    <Button name="Button_Buy1" MouseUp="BUY1" />
                    <Button name="Button_Buy2" MouseUp="BUY2" />
                    <Button name="Button_Buy3" MouseUp="BUY3" />
                    <Button name="Button_Buy4" MouseUp="BUY4" />
                    <Button name="Button_Buy5" MouseUp="BUY5" />
                </Container>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_StarCoin>
            
            <!-- SHOP -->
            <Popup_Shop name="Popup_Shop" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Content" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="NORMAL_SQRT">
                    <Container name="Container_CloseButton" autoAlign="FALSE">
                        <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE"/>
                    </Container>

                    <MovieClip name="MovieClip_Title" autoAlign="FALSE"/>

                    <Container name="Container_StarCoin" autoAlign="FALSE">
                        <Button name="Button_BuyCoins" MouseUp="BUY_COINS" />
                    </Container>

                    <Container name="Container_Tabs" autoAlign="FALSE">
                        <Container name="Container_PowerUpsTab">
                             <Button name="Button_Tab" MouseUp="TAB_POWERUPS" />

                            <MovieClip name="MovieClip_Loading"/>

                            <Container name="Container_SubscriptionInfo" visible="False" >
                                <Button name="Button_Renew0" MouseUp="BUY_SUBSCRIPTION_0" />
                            </Container>

                            <Button name="Button_Category0" MouseUp="CATEGORY0"/>
                            <Button name="Button_Category1" MouseUp="CATEGORY1"/>
                            <Button name="Button_Category2" MouseUp="CATEGORY2" />
                            <Button name="Button_Category3" MouseUp="CATEGORY3" />
                            <Button name="Button_Category4" MouseUp="CATEGORY4" />
                            <Button name="Button_Category5" MouseUp="CATEGORY5" />

                            <Button name="Button_Buy0" MouseUp="BUY0" />
                            <Button name="Button_Buy1" MouseUp="BUY1" />
                            <Button name="Button_Buy2" MouseUp="BUY2" />
                            <Button name="Button_Buy3" MouseUp="BUY3" />
                        </Container>

                        <Container name="Container_EnergyTab">
                            <Button name="Button_Tab" MouseUp="TAB_ENERGIES" />

                            <MovieClip name="MovieClip_Loading"/>

                             <Container name="Container_OutOfEnergy" visible="False" >
                                <Button name="Button_Check" MouseUp="CLOSE_ENERGY_NOTICE" scaleOnMouseOver="TRUE" />
                            </Container>

                            <Container name="Container_SubscriptionInfo" visible="False" >
                                <Button name="Button_Renew0" MouseUp="BUY_SUBSCRIPTION_0" />
                            </Container>

                            <Container name="Container_Bundle">
                                <Button name="Button_Subscription0" MouseUp="BUY_SUBSCRIPTION_0" />
                                <Button name="Button_Subscription1" MouseUp="BUY_SUBSCRIPTION_1" />
                                <Button name="Button_Subscription2" MouseUp="BUY_SUBSCRIPTION_2" />
                            </Container>

                            <Button name="Button_BuyFriends" MouseUp="BUY_ENERGY_FRIEND" />
                            <Button name="Button_Buy0" MouseUp="BUY_ENERGY_0" />
                            <Button name="Button_Buy1" MouseUp="BUY_ENERGY_1" />
                        </Container>
                    </Container>
                </Container>

                <MovieClip name="MovieClip_Bg" width="760" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER" />
            </Popup_Shop>
            
            <!-- INBOX -->
            <Popup_Inbox name="Popup_Inbox" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">

                <MovieClip name="MovieClip_ContentMask" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                <MovieClip name="MovieClip_Frame" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <Container name="Container_Content" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />

                    <Button name="Button_ScrollUp" MouseUp="UP" scaleOnMouseOver="TRUE" />
                    <Button name="Button_ScrollDown" MouseUp="DOWN" scaleOnMouseOver="TRUE" />
                    
                    <Button name="Button_SendGifts" MouseUp="SENDGIFTS" scaleOnMouseOver="FALSE" />
                    <Button name="Button_InviteFriends" MouseUp="INVITEFRIENDS" visible="false" scaleOnMouseOver="FALSE" />
                </Container>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
        	</Popup_Inbox>
        	
        	<!-- REQUESTS -->
        	<Popup_Requests name="Popup_Requests" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                <MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <Container name="Container_Content" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                	<Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
                    
                    <Container name="Container_FriendsType">
                        <Button name="Button_FriendTypeDropDown" MouseDown="FRIEND_TYPE_DROPDOWN"/>
                    </Container>
                    
                    <Button name="Button_ScrollUp" MouseUp="UP" scaleOnMouseOver="TRUE" />
                    <Button name="Button_ScrollDown" MouseUp="DOWN" scaleOnMouseOver="TRUE" />
                    
                </Container>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
			</Popup_Requests>
			
			<!-- TUTORIAL -->
            <Popup_Tutorial name="Popup_Tutorial" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <Container name="Container_Popup" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <MovieClip name="MovieClip_Title" visible="false"/>

                    <Container name="Container_CloseButton" autoAlign="false">
                        <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
                    </Container>

                    <MovieClip name="MovieClip_ContentMask" />
                    <Container name="Container_Animation" />
                </Container>

                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_Tutorial>
            
            <!-- TOURNAMENT RESULTS -->
            <Popup_TournamentResults name="Popup_TournamentResults" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
                <MovieClip name="MovieClip_ContentMask" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                <MovieClip name="MovieClip_Frame" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                
                <Container name="Container_Content" width="760" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                    <Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
                    <Container name="Container_Top_Battle"> </Container>
                    
                    <Container name="Container_Bottom_Battle">
                    	<Container name="Container_Bottom_Battle_Content" autoAlign="false">
                    		<Button name="Button_Claim" MouseUp="CLAIM" scaleOnMouseOver="TRUE" />
                    	</Container>
                    </Container>
                    
                    
                    <Container name="Container_Top_Results"></Container>
                    
                    <Container name="Container_Bottom_Results">
                    	<Container name="Container_Bottom_Results_Content" autoAlign="false">
                    		<Button name="Button_Challenge" MouseUp="CHALLENGE" scaleOnMouseOver="false" />
	                    	<Button name="Button_Celebrate" MouseUp="CELEBRATE" scaleOnMouseOver="false" />
	                    </Container>
                    </Container>
                    
                </Container>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
        	</Popup_TournamentResults>
        	
        	<Popup_PreviousResults name="Popup_PreviousResults" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
	            <MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                <MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>

                <Container name="Container_Content" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
                	<Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
                    <Button name="Button_ScrollUp" MouseUp="UP" scaleOnMouseOver="TRUE" />
                    <Button name="Button_ScrollDown" MouseUp="DOWN" scaleOnMouseOver="TRUE" />
                </Container>

                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
			</Popup_PreviousResults>
			
			<Popup_CrystalImprovements name="Popup_CrystalImprovements" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
				<MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
				<MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
            	 <Container name="Container_Content" width="940" height="570"  scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
                 <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>
            </Popup_CrystalImprovements>
			
			 <!-- GENERIC MESSAGE POPUP -->
            <Popup_GenericMessage name="Popup_GenericMessage" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
				<MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
				<MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
				<Container name="Container_Content" width="940" height="570" scaleH="true" scaleV="true" aspectRatioFixed="TRUE" scaleFunction="SQRT">
					<Textfield name="message"/>
					<Button name="Button_Ok" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
				</Container>
                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>				
            </Popup_GenericMessage>
            
            <!-- SHUTDOWN POPUP -->
            <Popup_Shutdown name="Popup_Shutdown" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="0" height="0">
				<MovieClip name="MovieClip_ContentMask" width="940" height="570" scaleH="false" scaleV="false" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
				<MovieClip name="MovieClip_Frame" width="940" height="570" scaleH="false" scaleV="false" aspectRatioFixed="TRUE" scaleFunction="SQRT"/>
				<Container name="Container_Content" width="940" height="570" scaleH="false" scaleV="false" aspectRatioFixed="TRUE" scaleFunction="SQRT">
					<Button name="Button_Redirect" MouseUp="NAVIGATE_OUT" scaleOnMouseOver="False"/>
					<Button name="Button_Close" MouseUp="CLOSE" scaleOnMouseOver="TRUE" />
				</Container>
                <MovieClip name="MovieClip_Blocker" width="940" height="570" scaleH="true" scaleV="true" alignH="CENTER" alignV="CENTER"/>				
            </Popup_Shutdown>
        	
        </Popups>
</UIDescription>