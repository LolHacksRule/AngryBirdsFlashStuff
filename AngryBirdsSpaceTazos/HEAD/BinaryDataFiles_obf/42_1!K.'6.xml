		<Button name="Checkbox_RememberMe" MouseUp="CHECKBOX_CHANGE"/>

		<Button name="Button_ForgotPassword" MouseUp="FORGOT_PASSWORD" scaleOnMouseOver = "True"/>

		<MovieClip name="MovieClip_ErrorLoginMarkEmail" Enabled="False" visible="False"/>
		<MovieClip name="MovieClip_ErrorLoginMarkPassword" Enabled="False" visible="False"/>
	</Container>

