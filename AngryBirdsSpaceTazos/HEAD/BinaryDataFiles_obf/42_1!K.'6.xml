		<TextField name="Text_Password_Register"/>
		<TextField name="TextField_RememberMe_Register"/>
		<TextField name="TextField_PasswordAgain"/>
		<TextField name="TextField_Nickname"/>
		<TextField name="TextField_FirstNameField_Register"/>
		<TextField name="TextField_LastNameField_Register"/>
		<TextField name="TextField_DD" />
		<TextField name="TextField_MM" />
		<TextField name="TextField_YYYY" />
		<TextField name="TextField_MonthField_Register"/>
		<TextField name="TextField_DayField_Register"/>
		<TextField name="TextField_YearField_Register"/>
		<TextField name="TextField_IWantToReceiveAds"/>
		<TextField name="TextField_IWantToReceiveExtAds"/>
		<TextField name="TextField_ZipcodeField_Register"/>



		<Button name="Checkbox_RememberMe_Register" MouseUp="CHECKBOX_CHANGE"/>
		<Button name="Checkbox_Marketing" MouseUp="CHECKBOX_MARKETING_CHANGE"/>
		<Button name="Checkbox_ExtMarketing" MouseUp="CHECKBOX_EXT_MARKETING_CHANGE"/>
