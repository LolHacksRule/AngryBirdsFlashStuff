<UIDescription>
	<Views>
		
		
		
		<View_ForgotPassword name="ViewForgotPassword">
			<TextField name="TextField_UserNameField" tabIndex="1"/>
			
			<Button name="Button_SendPassword" MouseUp="SEND_PASSWORD"/>
			<Button name="Button_Cancel" MouseUp="CANCEL"/>
		</View_ForgotPassword>
		
		
		<!-- VIEW LOGIN -->
		<View_Login name="ViewLogin">
			<TextField name="TextField_UserNameField" tabIndex="1" />
			<TextField name="TextField_PasswordField" tabIndex="2" />
			
			<Button name="Button_Submit" MouseUp="SUBMIT"/>
			<Button name="Button_Register" MouseUp="REGISTER"/>
			
			<Button name="Checkbox_RememberMe" MouseUp="CHECKBOX_CHANGE"/>
			
			<Button name="Button_ForgotPassword" MouseUp="FORGOT_PASSWORD" />
			
			<MovieClip name="WaitingPopup" visible="false" />
		</View_Login>
		
		<View_Register name="ViewRegister">
			<TextField name="TextField_UserNameField" tabIndex="1" />
			<TextField name="TextField_PasswordField" tabIndex="2" />
			<TextField name="TextField_PasswordFieldRepeat" tabIndex="3" />
			<TextField name="TextField_EmailField" tabIndex="4" />
			
			<TextField name="TextField_AgeField" tabIndex="5" />
						
			<TextField name="TextField_FullNameField" tabIndex="6" />
			<TextField name="TextField_ZipCodeField" tabIndex="7" />
			
			
			<Button name="Button_Male" MouseUp="MALE"/>
			<Button name="Button_Female" MouseUp="FEMALE"/>
			
			<Button name="Button_Cancel" MouseUp="CANCEL"/>
			<Button name="Button_Submit" MouseUp="SUBMIT"/>
			
			<Button name="Checkbox_Marketing" MouseUp="CHECKBOX_CHANGE"/>
			
			<MovieClip name="WaitingPopup" visible="false" />
		</View_Register>
		
		<!--  VIEW MENU    -->				
		<View_LevelStart name="ViewLevelStart" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_UI">
				<Button name="Button_Play"  MouseUp="PLAY_LEVEL" alignH="ALING_CENTER" alignV="ALING_CENTER"/>
				<Button name="Button_OpenMenu" MouseUp="OPEN_MENU" alignH="ALING_LEFT" alignV="ALING_BOTTOM"/>
				<Container name="Container_MenuButtons" visible="False" alignH="ALING_LEFT" alignV="ALING_BOTTOM">
					<Button name="Button_ToggleSound" MouseUp="TOGGLE_SOUNDS"/>
					<Button name="Button_Credits" MouseUp="OPEN_CREDITS"/>
					<Button name="Button_ToggleParticles" MouseUp="TOGGLE_PARTICLES"/>
					<MovieClip name="MovieClip_SoundsOff" visible="False" enabled="False"/>
					<MovieClip name="MovieClip_ParticlesOff" visible="False" enabled="False"/>
					<Button name="Button_Fullscreen" MouseUp="TOGGLE_FULLSCREEN"/>
				</Container>
				<Button name="Button_AvatarCreator" MouseUp="OPEN_AVATAR_CREATOR" visible="False" />
			</Container>
			
			<Popup name="Popup_Credits" visible="False" fromLibrary="True" overlay="True" >
				<TextField name="TextField_Credits" />
				<MovieClip name="MovieClip_CreditsText" />
				<Button name="Popup_Credits_Close" MouseUp="POPUP_CREDITS_CLOSE" />
			</Popup>
			
		</View_LevelStart>
		
		
		<View_Credits name="ViewCredits" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">      
			<Container name="Container_Credits">
				<TextField name="TextField_Version_Number" text="1.0.0 placeholder" enabled="True" />
			</Container>
			
			<Button name="Button_Back" MouseUp="CREDITS_CLOSE_BUTTON" alignH="LEFT" alignV="BOTTOM" aspectRatioFixed="TRUE"/>
		</View_Credits>
		
		
		<View_LevelSelection name="ViewLevelSelection">
		
			<Container name="LogoutConfirmationPopup" visible="false">
				<Button name="Button_Si" MouseUp="LOGOUT_SI" />
				<Button name="Button_No" MouseUp="LOGOUT_NO" />
			</Container>
		
			<Container name="InsertCodePopup" visible="false">
				<TextField name="TextField_Code" />
				<Button name="Button_Cancel" MouseUp="CANCEL" />
				<Button name="Button_SendCode" MouseUp="SEND_CODE" />
				<MovieClip name="WaitingPopup" visible="false" />
			</Container>
			
			
			
			<Button name="Button_LeftMenuOpen"  MouseUp="OPEN_LEFT_MENU" />
			<Button name="Button_RightMenuOpen" MouseUp="OPEN_RIGHT_MENU" />
			
			<Container name="Container_MenuLeftButtons" visible="False">
				<Button name="Button_MenuSound" MouseUp="SOUNDS_BUTTON"/>
				<Button name="Button_MenuCredits" MouseUp="CREDITS_BUTTON"/>
				<MovieClip name="MovieClip_SoundsOff" visible="False" enabled="False"/>
			</Container>
				
			<Container name="Container_MenuRightButtons" visible="False">
				<Button name="Button_MenuTwitter" MouseUp="TWITTER"/>
				<Button name="Button_MenuFacebook" MouseUp="FACEBOOK"/>
			</Container>
		
			<Button name="Button_Logout" MouseUp="LOGOUT" />
		
			<Button name="Button_ChromePage" MouseUp="NAVIGATE_CHROME" />
			<Button name="Button_FacebookPage" MouseUp="NAVIGATE_FACEBOOK" />
		</View_LevelSelection>

		<View_ChapterSelection name="ViewChapterSelection">
			<Button name="Button_Back" MouseUp="Back" />
			<Button name="Button_Prev" MouseUp="Prev" />
			<Button name="Button_Next" MouseUp="Next" />
		</View_ChapterSelection>
		
		
		<!--  VIEW LEVEL PLAY     -->
		<View_LevelPlay name="ViewLevelPlay">    
				<Container name="Container_HiscoreContainer" enabled="False" >
						<TextField name="TextField_Hiscore" text="0" enabled="False"/>
						<TextField name="TextField_CurrentScore" text="0" enabled="False"/>
				</Container>
				
				<!-- <Button name="MovieClip_FeedBar" MouseUp="OPEN_FEED_DIALOG" enabled="True" /> -->
				
				<Button name="Button_Pause" MouseUp="PAUSE" MouseOver="PLAY_VIEW_ROLLOVER" MouseOut="PLAY_VIEW_ROLLOUT"/>
				<Button name="Button_Restart" MouseUp="RESTART_LEVEL" MouseOver="PAN_LEFT_OVER" MouseOut="PAN_LEFT_OUT" />
		</View_LevelPlay> 
				
		<!--  VIEW LEVEL LOAD    -->				
		<View_LevelLoad name="ViewLevelLoad">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
				<!-- <MovieClip name="" /> --> 
		</View_LevelLoad>
		
		<!--  VIEW GAME PAUSE     		
		<View_LevelPause name="ViewLevelPause" isOverlay="False" overlayColor="0x000000" overlayAlpha="0.5">
			<Button name="Button_Pause_Background" MouseUp="RESUME_LEVEL" isBehind="True" />
			<Container name="Pause_Slider" animateOnActivation="True">
				<Container name="Menu_Pause_Anim">   
					<Button name="Button_Resume" MouseUp="RESUME_LEVEL" />
					<Button name="Button_Sound" MouseUp="TOGGLE_SOUNDS"/> 	
					<Button name="Button_Replay" MouseUp="RESTART_LEVEL" />
					<Button name="Button_Help" />
					<Button name="Button_Back" MouseUp="BACK"/>
					<Button name="Button_ToggleParticles" MouseUp="TOGGLE_PARTICLES" />
					<Button name="Button_Fullscreen" MouseUp="TOGGLE_FULLSCREEN" />
					<MovieClip name="MovieClip_SoundsOff" visible="False" enabled="False" />
					<MovieClip name="MovieClip_ParticlesOff" visible="False" enabled="False" />
				</Container>
			</Container>
		</View_LevelPause>
		-->
		<!--  VIEW GAME PAUSE -->		
		<View_LevelPause name="ViewLevelPause">
			<MovieClip name="MovieClip_DarkBG"/>
			<Container name="Container_PauseMenu">
				<TextField name="TextField_LevelName" text="22-22" enabled="True" />
				<Button name="Button_Resume" MouseUp="RESUME_LEVEL" />
				<Button name="Button_Replay" MouseUp="RESTART_LEVEL" />
				<Button name="Button_Menu" MouseUp="MENU"/>
				<Button name="Button_Help" MouseUp="HELP"/>
				<Button name="Button_Sound" MouseUp="TOGGLE_SOUNDS"/> 	
				<MovieClip name="MovieClip_SoundsOff" visible="False" enabled="False" />
			</Container>
		</View_LevelPause>
		
		<!--  LEVEL END VIEW    -->
		<View_LevelEnd name="ViewLevelEnd2" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
				<Button name="Button_Play"  MouseUp="PLAY_NEXT" />
				<Button name="Button_Restart"  MouseUp="RESTART_LEVEL" />
				<Button name="Button_Back"  MouseUp="BACK" />
				<MovieClip name="MovieClip_AvatarPlaceholder" visible="False" />
				<MovieClip name="MovieClip_AvatarPlaceholderList" visible="False" />
				<!-- <Button name="Button_Leaderboard"  MouseUp="OPEN_LEADERBOARD" /> -->
				<TextField name="TextField_LevelRank" text="0" enabled="False" />
				<TextField name="TextField_LevelScore" text="0" enabled="False" />
				<TextField name="TextField_LevelScore_1" text="0" enabled="False" />
				<TextField name="TextField_LevelScore_2" text="0" enabled="False" />
				<TextField name="TextField_LevelScore_3" text="0" enabled="False" />
				<TextField name="TextField_LevelScore_4" text="0" enabled="False" />
				<TextField name="TextField_LevelHighScore" text="0" enabled="False" />
				<TextField name="TextField_LevelName" text="22-22" enabled="False" />
				<MovieCLip name="MovieClip_NewHighScore" />
				<MovieClip name="MovieClip_Stars_NewScore" />
				<MovieClip name="MovieClip_Stars_HighScore" />
		</View_LevelEnd>
		<!--  LEVEL END VIEW    -->

		<!--  NEW LEVEL END VIEW    -->
		<View_LevelEndRio name="ViewLevelEnd3" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">
			<Container name="Container_LevelEndStripe">

				<Button name="Button_Menu"  MouseUp="MENU"/>
				<Button name="Button_Replay"  MouseUp="REPLAY"/>
				
				<MovieClip name="MovieClip_StarLeft" />
				<MovieClip name="MovieClip_StarCenter" />
				<MovieClip name="MovieClip_StarRight" />
				
				
				<TextField name="TextField_LevelEndScore" enabled="True" />
				
				<TextField name="TextField_NewHighScore" enabled="True" visible="false" />

				<TextField name="TextField_BestScore" enabled="True" visible="false" />
				<MovieClip name="MovieClip_BestScoreStars" enabled="True" visible="false" />

				
				<MovieClip name="MovieClip_NewHighScoreBadge" visible="false"/>
			</Container>
			
		</View_LevelEndRio>
		<!--  NEW LEVEL END VIEW    -->				
		
		<!--  EAGLE LEVEL END VIEW    -->
		<View_LevelEndEagle name="ViewLevelEndEagle" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">
			<Container name="Container_LevelEndEagleStripe">
				<Button name="Button_Menu"  MouseUp="MENU" />
				<Button name="Button_Replay"  MouseUp="REPLAY" />
<!--				<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" />  -->
				
				<MovieClip name="MovieClip_EagleMeterEmpty" visible="false"/>
				<MovieClip name="MovieClip_EagleMeterFill" visible="false"/>
				<MovieClip name="MovieClip_EagleMeterEffect" visible="false"/>
				
				<TextField name="TextField_EaglePercentage" text="0%" enabled="True" visible="true"/>
			</Container>
		</View_LevelEndEagle>
		<!--  EAGLE LEVEL END VIEW    -->				

		
		<View_LevelEndFail name="ViewLevelEndFail" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
				<Button name="Button_Play"  MouseUp="PLAY_NEXT" />
				<Button name="Button_Restart"  MouseUp="RESTART_LEVEL" />
				<TextField name="TextField_FriendScore" text="25200" enabled="False" />
		</View_LevelEndFail>
		
		<View_LevelEndFail3 name="ViewLevelEndFail3" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_LevelEndStripe">
				<Button name="Button_Menu"  MouseUp="MENU" />
				<Button name="Button_Replay"  MouseUp="REPLAY" />
<!--			<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" />  -->
				
				<MovieClip name="MovieClip_LevelEndFailedPig" visible="true"/>
			</Container>
		</View_LevelEndFail3>
		
		
		<!-- LEADERBOARD VIEW -->
		<View_Leaderboard name="ViewLeaderboard" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<!-- <Button name="Button_Back"  MouseUp="BACK" /> -->
			<Button name="Button_Play"  MouseUp="PLAY_NEXT" />
			<Button name="Button_Restart"  MouseUp="RESTART_LEVEL" />
		</View_Leaderboard> 
		
		
		<!-- LEADERBOARD VIEW -->
		<View_FlashGoes11 name="ViewFlashGoes11" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			
		</View_FlashGoes11>
		
		
		<!-- LEVEL EDITOR VIEW -->
		<View_LevelEditor name="ViewLevelEditor">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_EditorTop">
				<Button name="Button_ViewBird" MouseUp="TOP_BAR_BIRD_VIEW" Tooltip="Go to birds" /> 	
				<Button name="Button_ViewBird_BirdAlone" MouseUp="TOP_BAR_BIRD_VIEW" Tooltip="Go to birds" /> 	
				<Button name="Button_ViewPig" MouseUp="TOP_BAR_PIG_VIEW"  Tooltip ="Go to pigs"/>
				<Button name="Button_ViewPig_PigAlone" MouseUp="TOP_BAR_PIG_VIEW"  Tooltip ="Go to pigs"/>
				<Button name="Button_ZoomIn" MouseUp="TOP_BAR_ZOOM_IN" Tooltip ="Zoom In" />
				<Button name="Button_ZoomOut" MouseUp="TOP_BAR_ZOOM_OUT" Tooltip ="Zoom Out" />
				<Button name="Button_Hand" MouseUp="TOOL_CAMERA"  Tooltip="Drag Camera"/> 
				<Button name="Button_ToolSelect" MouseUp="TOOL_SELECT" Tooltip = "Selection Tool (m)"  />
				<Button name="Button_ToolUndo" MouseUp="ACTION_UNDO" Tooltip="Undo (z)"/> 
				<Button name="Button_ToolDoPhysics" MouseUp="DO_PHYSICS" Tooltip="Toggle Physics (g)" />
				<Button name="Button_ToggleSnap" MouseUp="TOGGLE_SNAP" Tooltip="Toggle snap" /> 
				<Button name="Button_BottomUI_LevelTheme" MouseUp="BOTTOM_SELECT_THEME" Tooltip ="Change Background"/>
				<Button name="Button_ExtendLevel" MouseUp="BOTTOM_EXTEND_LEVEL" Tooltip ="Expand level"/>
				<Button name="Button_Stamp" MouseUp="STAMP" Tooltip="Stamp" /> 
				
				<Container name="Container_Editor_BlockLimit" enabled="False" visible="False">
					<TextField name="Textfield_BlockLimit" enabled="False"/>
					<MovieClip name="MovieClip_BlockLimit_Bg" enabled="False" />
				</Container>
			</Container>
			
			<Button name="Button_TestPlay" MouseUp="PLAY_LEVEL" Tooltip="Test Level"/>
							
			<Container name="Container_Editor_CategoryBar" MouseOver="EDITOR_REPEATER_TABS_MOUSEOVER" MouseOut="EDITOR_REPEATER_TABS_MOUSEOUT" enabled="True" visible="False">
				<Repeater name="Repeater_Tabs"  button="Repeater_Button_Editor_Tabs" buttonIconContainer="ImageContainer_Tabs" buttonAlign="Bottom" /> <!--  button parameter must be identical(case sensitive) with export name of the button clip which will be used as a base for each selection      -->
			</Container>
			
			<Repeater name="Repeater_Items"  button="Repeater_Button_Editor_Items" buttonIconContainer="ImageContainer_Item" buttonAlign="Bottom" GlowFilter="0xFFFFFF" MouseOver="EDITOR_REPEATER_ITEMS_MOUSEOVER" MouseOut="EDITOR_REPEATER_ITEMS_MOUSEOUT" enabled="True" visible="False">
			</Repeater>
			
			<Container name="Container_Toolbar">
				<Button name="Button_NudgeUp" MouseUp="NUDGE_UP" visible="True" Tooltip = "Nudge"/>
				<Button name="Button_NudgeDown" MouseUp="NUDGE_DOWN" visible="True" Tooltip = "Nudge"/>
				<Button name="Button_NudgeLeft" MouseUp="NUDGE_LEFT" visible="True" Tooltip = "Nudge"/>
				<Button name="Button_NudgeRight" MouseUp="NUDGE_RIGHT" visible="True" Tooltip = "Nudge"/>
				<Button name="Button_RotateClockwise" MouseUp="ROTATE_CLOCKWISE" visible="True" Tooltip = "Rotate"/>
				<Button name="Button_RotateCounterClockwise" MouseUp="ROTATE_COUNTERCLOCKWISE" visible="True" Tooltip = "Rotate"/> 
				<Button name="Button_FlipHorizontally" MouseUp="FLIP_HORIZONTALLY" visible="True" Tooltip = "Flip"/>
				<Button name="Button_FlipVertically" MouseUp="FLIP_VERTICALLY" visible="True" Tooltip = "Flip"/> 
				
				<Button name="Button_ToolDelete" MouseUp="TOOL_DELETE" visible="True" Tooltip = "Eraser Tool (del)" /> 
			</Container>
			
			<Popup name="Popup_Editor_LevelExtend" enabled="True" visible="False" fromLibrary="True" isOverlay="True">
				<MovieClip name="MovieClip_LevelExtend_Image" />
				<Container name="Container_LevelExtend_Extension1" visible="False">
					<Button name="Button_LevelExtend_Confirm1" MouseDown="POPUP_LEVELEXTEND_CONFIRM" />
				</Container>
				
				<Container name="Container_LevelExtend_Extension2" visible="False">
					<Button name="Button_LevelExtend_Confirm2" MouseDown="POPUP_LEVELEXTEND_CONFIRM" />
				</Container>
				
				<Container name="Container_LevelExtend_Extension3" visible="False">
					<Button name="Button_LevelExtend_Confirm3" MouseDown="POPUP_LEVELEXTEND_CONFIRM" />
				</Container>
				
				<Button name="Button_LevelExtend_Exit" MouseUp="POPUP_LEVELEXTEND_CLOSE" />
			</Popup>
		</View_LevelEditor>
	</Views>
	
	<Components>
		<Container name="Component_LevelSelection">
			<MovieClip name="MovieClip_LevelSelectionLogos"/>
		</Container>
	</Components>
</UIDescription>