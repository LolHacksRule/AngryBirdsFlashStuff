		<ViewLevelSelectionCustomChocapic name="ViewLevelSelectionCustomChocapic" width="980" height="570" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" aspectRatioFixed="TRUE" alignV="CENTER" alignH="CENTER" scaleV="True" scaleH="True">
			<Container name="Container_Chapter1">
				<Button name="Button_Level1" MouseUp="C1_LEVEL_1" />
				<Button name="Button_Level2" MouseUp="C1_LEVEL_2" />
				<Button name="Button_Level3" MouseUp="C1_LEVEL_3" />
