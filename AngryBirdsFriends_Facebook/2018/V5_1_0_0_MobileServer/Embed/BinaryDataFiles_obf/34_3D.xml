<UIDescription>
	<Views>
		<PopupView_Error name="Errorbox" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Error>
	
		<PopupView_Warning name="Warningbox" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		</PopupView_Warning>

		<PopupView_FullGame name="FullGameBox" fromLibrary="True" overlay="True" width="398" height="270" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
            <Container name="Container_Popup" visible="false" />
        </PopupView_FullGame>

        <PopupView_FullScreen name="FullScreenBox" fromLibrary="True" overlay="True" width="398" height="270" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
            <Container name="Container_Popup" visible="false" />
        </PopupView_FullScreen>


        <View_FacebookLevelPause name="ViewFacebookLevelPause" width="1024" height="768">
			<MovieClip name="MovieClip_DarkBG" scaleH="True" scaleV="True" visible="true"/>

			<Container name="Container_PauseMenu" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
				<MovieClip name="MovieClip_PauseMenuBG"/>
				<TextField name="TextField_LevelName" text="22-22" enabled="True" />
				<TextField name="TextField_ChapterName" enabled="True" />
				<Button name="Button_Resume" MouseUp="RESUME_LEVEL" scaleOnMouseOver="FALSE"/>
				<Button name="Button_Replay" MouseUp="RESTART_LEVEL" scaleOnMouseOver="FALSE"/>
			</Container>

			<Container name="Container_Screen" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE" alignH="CENTER">
                <Container name="Container_FullGame">
                    <Button name="Button_FullGame" MouseUp="FULL_GAME" scaleOnMouseOver="FALSE"/>
                </Container>
            </Container>
		</View_FacebookLevelPause>
	

		<View_FacebookLevelPlay name="ViewFacebookLevelPlay" width="1024" height="768">
				<Container name="Container_HiscoreContainer" enabled="True" alignH="RIGHT" visible="True" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
						<TextField name="TextField_HighScore" text="0" enabled="True"/>
				</Container>
			<Container name="Container_LevelscoreContainer" enabled="True" alignH="RIGHT" visible="True" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
				<TextField name="TextField_CurrentScore" text="0" enabled="True"/>
			</Container>

			<Container name="Container_ZoomButtons" enabled="False" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE" alignV="BOTTOM" alignH="LEFT">
				<Button name="Button_ZoomIn" MouseUp="ZOOM_IN"/>
				<Button name="Button_ZoomOut" MouseUp="ZOOM_OUT"/>
				<Button name="Button_Magnify"/>
			</Container>

			<Container name="Container_Buttons" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
					<Button name="Button_Pause" MouseUp="PAUSE" alignH="LEFT"/>
					<Button name="Button_Powerup" MouseUp="POWERUP" alignH="LEFT"/>

			</Container>

            <Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON" alignH="RIGHT" alignV="BOTTOM" scaleFunction="SQRT" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE"/>

		</View_FacebookLevelPlay>

		<View_LevelEndEmbedRio name="ViewLevelEndRio" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
				<MovieClip name="MovieClip_LevelEndStripeBG" scaleV="FALSE" alignV="CENTER"/>

				<!--<Button name="Button_Menu"  MouseUp="" scaleOnMouseOver="TRUE"/>-->
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<!--<Button name="Button_NextLevel"  MouseUp="" scaleOnMouseOver="TRUE"/>
				<Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="False" scaleOnMouseOver="TRUE"/>-->


				<MovieClip name="MovieClip_StarLeft" />
				<MovieClip name="MovieClip_StarCenter" />
				<MovieClip name="MovieClip_StarRight" />


				<TextField name="TextField_LevelEndScore" enabled="True" />
				<TextField name="TextField_LevelEndScoreEffects" enabled="True" />
                <MovieClip name="MovieClip_ScoreToBeatTitle" enabled="True" />
                <TextField name="TextField_ScoreToBeat" enabled="True" />

                <!--<TextField name="TextField_NewHighScore" enabled="True" visible="false" />-->

				<!--<TextField name="TextField_BestScore" enabled="True" visible="false" />-->
				<!--<MovieClip name="MovieClip_BestScoreStars" enabled="True" visible="false" />
				<MovieClip name="MovieClip_ResultMEFeather" enabled="True" visible="true" />-->


				<MovieClip name="MovieClip_NewHighScoreBadge" visible="false"/>
				<Button name="Button_FullGame" MouseUp="FULL_GAME" scaleOnMouseOver="FALSE"/>
			</Container>

			<!--<Button name="Button_Watch_Replay"  MouseUp="WATCH_REPLAY" alignH="RIGHT" scaleOnMouseOver="TRUE"/>-->
			<!--<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>-->
		</View_LevelEndEmbedRio>

		<View_LevelEndEmbedFailRio name="ViewLevelEndFailRio" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
				<MovieClip name="MovieClip_LevelEndStripeFailBG" scaleV="FALSE" alignV="CENTER"/>/>

				<!--<Button name="Button_Menu"  MouseUp="MENU" scaleOnMouseOver="TRUE"/>-->
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<!--<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" scaleOnMouseOver="TRUE"/>
				<Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="false" scaleOnMouseOver="TRUE"/>
				<Button name="Button_MightyEagle"  MouseUp="SHOP" visible="false" scaleOnMouseOver="TRUE"/>-->

				<MovieClip name="MovieClip_LevelEndFailedPig" visible="true"/>
				<Button name="Button_FullGame" MouseUp="FULL_GAME" scaleOnMouseOver="FALSE"/>
			</Container>

			<!--<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>-->
		</View_LevelEndEmbedFailRio>


	</Views>

	<Components>
		
	</Components>
</UIDescription>