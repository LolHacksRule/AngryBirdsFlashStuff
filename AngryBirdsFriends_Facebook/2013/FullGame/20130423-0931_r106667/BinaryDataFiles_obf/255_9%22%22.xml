<UIDescription>
	<Views>
	<!-- BRANDED TOURNAMENT STUFF -->
	
	
	<PopupView_EasterRaffleIntro name="EasterRaffleIntroductionPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER"/>
	
	<PopupView_EasterRaffleEnter name="EasterRaffleEnterPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
		<MovieClip name="MovieClip_Popup_Loading" visible="true"/>
		
		<TextField name="TextField_Email_Input"/>
		
		
		<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="true"/>
		<Button name="Enter_Button" MouseUp="SUBMIT_NAME_TO_COMPETITION" visible="true"/>
		<Button name="Button_TermsAndConditions" MouseUp="TERMS_AND_CONDITIONS" visible="true" scaleOnMouseOver="TRUE"/>
		<Button name="Checkbox_TermsAndConditions" MouseUp="CHECKBOX_CHANGE"/>
		
	</PopupView_EasterRaffleEnter>
	
	
	<PopupView_NewTournamentBranded name="TournamentNewBrandedPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER"/>
		
	<PopupView_BundleBranded name="TournamentBundleBrandedPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER"/>
	
 	<PopupView_LastChristmasBundle name="TournamentLastChristmasBundlePopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER"/>
		
	<PopupView_ExtraBirdGift name="TournamentPopupExtraGift" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER"/>
	
	<!-- TOURNAMENT STUFF -->
		<PopupView_TournamentLastResultsBranded name="TournamentLastResultsPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<Button name="ClaimPrizeButton" MouseUp="CLAIM_PRIZE"/>
			<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="false"/>
			<Button name="ShareTournamentButton" MouseUp="SHARE_TOURNAMENT" visible="false"/>
			
			<Container name="GiftCarouselContainer" visible="false">
				<MovieClip name="mcCount"/>
				<MovieClip name="mcReward"/>
			</Container>
			
			<MovieClip name="OwnAvatarHolder"/>
			<MovieClip name="OwnAvatarStar"/>
			<MovieClip name="GiftBox"/>
			
			<Container name="PodiumContainer">
				<MovieClip name="SilverAvatarHolder"/>
				<MovieClip name="GoldAvatarHolder"/>
				<MovieClip name="BronzeAvatarHolder"/>
				
				<MovieClip name="SilverTrophy"/>
				<MovieClip name="GoldTrophy"/>
				<MovieClip name="BronzeTrophy"/>
				
				<TextField name="TextField_Podium1"/>
				<TextField name="TextField_Podium2"/>
				<TextField name="TextField_Podium3"/>
				
				<TextField name="firstPlaceText"/>
				<TextField name="secondPlaceText"/>
				<TextField name="thirdPlaceText"/>
			</Container>
			
			<Container name="CombinedBackground">
				<MovieClip name="FireworksArea"/>
			</Container>
			
		</PopupView_TournamentLastResultsBranded>
	
		<PopupView_BrandedTournamentLastResults name="TournamentLastResultsPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<Button name="ClaimPrizeButton" MouseUp="CLAIM_PRIZE"/>
			<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="false"/>
			<Button name="ShareTournamentButton" MouseUp="SHARE_TOURNAMENT" visible="false"/>
			
			<Container name="GiftCarouselContainer" visible="false">
			</Container>
			
			<MovieClip name="OwnAvatarHolder"/>
			<MovieClip name="OwnAvatarStar"/>
			<MovieClip name="GiftBox"/>
			
			<Container name="PodiumContainer">
				<MovieClip name="SilverAvatarHolder"/>
				<MovieClip name="GoldAvatarHolder"/>
				<MovieClip name="BronzeAvatarHolder"/>
				
				<MovieClip name="SilverTrophy"/>
				<MovieClip name="GoldTrophy"/>
				<MovieClip name="BronzeTrophy"/>
				
				<TextField name="TextField_Podium1"/>
				<TextField name="TextField_Podium2"/>
				<TextField name="TextField_Podium3"/>
				
				<TextField name="firstPlaceText"/>
				<TextField name="secondPlaceText"/>
				<TextField name="thirdPlaceText"/>
			</Container>
			
			<Container name="CombinedBackground">
				<MovieClip name="FireworksArea"/>
			</Container>
			
		</PopupView_BrandedTournamentLastResults>
		
		
		<PopupView_BrandedTournamentContestEnter name="TournamentCompetitionPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<MovieClip name="MovieClip_Popup_Loading" visible="true"/>
			
			<TextField name="TextField_Email_Input"/>
			
			
			<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="true"/>
			<Button name="Enter_Button" MouseUp="SUBMIT_NAME_TO_COMPETITION" visible="true"/>
			<Button name="Button_TermsAndConditions" MouseUp="TERMS_AND_CONDITIONS" visible="true" scaleOnMouseOver="TRUE"/>
			<Button name="Checkbox_TermsAndConditions" MouseUp="CHECKBOX_CHANGE"/>
			
		</PopupView_BrandedTournamentContestEnter>
		
		<PopupView_TournamentTermsAndConditions name="TournamentTermsAndConditionsPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="true"/>
			
			<MovieClip name="TermsAndConditions_Text"/>
			
			<Container name="TermsScrollBar">
				<Button name="TermsScrollBar_Button" MouseDown="SCROLL_DOWN" MouseUp="SCROLL_UP"/>
			</Container>
		</PopupView_TournamentTermsAndConditions>
		
		<PopupTournamentShopPopupBranded name="TournamentShopPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<Button name="Button_Shop_Bundle_Buy" MouseUp="Button_Shop_Bundle_Buy" scaleOnMouseOver="TRUE" />
			
			<Button name="Button_Buy_ExtraBird_Today" MouseUp="Button_Buy_ExtraBird_Today" scaleOnMouseOver="TRUE" />
			<Button name="Button_Buy_ExtraBird_Tournament" MouseUp="Button_Buy_ExtraBird_Tournament" scaleOnMouseOver="TRUE" />
			
			<Button name="Button_Buy_Tnt_Today" MouseUp="Button_Buy_Tnt_Today" scaleOnMouseOver="TRUE" />
			<Button name="Button_Buy_Tnt_Tournament" MouseUp="Button_Buy_Tnt_Tournament" scaleOnMouseOver="TRUE" />
			
			<Button name="Button_UnlockNext" MouseUp="Button_UnlockNext" scaleOnMouseOver="TRUE" />
			<Button name="Button_UnlockAll" MouseUp="Button_UnlockAll" scaleOnMouseOver="TRUE" />
			
		</PopupTournamentShopPopupBranded>
		
		<PopupView_TournamentInfo name="TournamentInfoPopupBranded" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER" alignV="CENTER">
			<Button name="Button_Close" MouseUp="CLOSE_POPUP" visible="true"/>
			<TextField name="Info_Text"/>
		</PopupView_TournamentInfo>
		
		<View_BrandedTournamentPrevious name="TournamentPreviousViewBranded" width="1188" height="571">
			<Container name ="PreviousWeekTextContainer" alignH="CENTER" alignV="TOP" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP">
				<TextField name="TournamentTitleTextField"/>
			</Container>
			<Container name="CombinedBackground" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP" alignH="CENTER" alignV="CENTER">
				<MovieClip name="FireworksArea"/>
			</Container>
			
			<Container name="PodiumContainer" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP">
				<MovieClip name="SilverAvatarHolder"/>
				<MovieClip name="GoldAvatarHolder"/>
				<MovieClip name="BronzeAvatarHolder"/>
				
				<MovieClip name="SilverTrophy"/>
				<MovieClip name="GoldTrophy"/>
				<MovieClip name="BronzeTrophy"/>
				
				<TextField name="TextField_Podium1"/>
				<TextField name="TextField_Podium2"/>
				<TextField name="TextField_Podium3"/>
				
				<TextField name="firstPlaceText" text="5"/>
				<TextField name="secondPlaceText"/>
				<TextField name="thirdPlaceText"/>
				
			</Container>
			
			<Container name="ShelfContainer" alignH="CENTER" alignV="BOTTOM" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP">
				<TextField name="GoldTrophiesTextfield"/>
				<TextField name="SilverTrophiesTextfield"/>
				<TextField name="BronzeTrophiesTextfield"/>
			</Container>
			
			<Button name="Button_Back" MouseUp="BACK" alignH="LEFT" alignV="BOTTOM"/>
			<Button name="Button_CurrentTournament" MouseUp="CURRENT_TOURNAMENT" alignH="RIGHT" alignV="BOTTOM"/>
		</View_BrandedTournamentPrevious>
		
		<!--  NEW LEVEL END VIEW    -->
		<View_TournamentBrandedLevelEnd name="TournamentBrandedViewLevelEnd" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER">
				<MovieClip name="MovieClip_LevelEndStripeBG" scaleV="True" alignV="CENTER"/>
				
				<Button name="Button_Menu"  MouseUp="MENU" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" scaleOnMouseOver="TRUE"/>
				<Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="False" scaleOnMouseOver="TRUE"/>
				
				<Button name="Button_FreePowerups"  MouseUp="FREE_POWERUPS" scaleOnMouseOver="TRUE" visible="True"/>
				
				<Button name="Button_Share_Default"  MouseUp="SHARE_DEFAULT" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Share_Crown"  MouseUp="SHARE_CROWN" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Share_Stars"  MouseUp="SHARE_STARS" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Share_ME"  MouseUp="SHARE_ME" scaleOnMouseOver="TRUE" visible="False"/>
				
				<Button name="Button_Embed"  MouseUp="EMBED" scaleOnMouseOver="TRUE" visible="True"/>
				
				<Button name="Button_Unlocked_Diamond" MouseUp="OPEN_AVATAR" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Unlocked_Gold" MouseUp="OPEN_AVATAR" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Unlocked_Bronze" MouseUp="OPEN_AVATAR" scaleOnMouseOver="TRUE" visible="False"/>
				<Button name="Button_Unlocked_Silver" MouseUp="OPEN_AVATAR" scaleOnMouseOver="TRUE" visible="False"/>
				
				<MovieClip name="mcGiftIcon" visible="false"/>
				
				<MovieClip name="MovieClip_StarLeft" />
				<MovieClip name="MovieClip_StarCenter" />
				<MovieClip name="MovieClip_StarRight" />
				
				
				<TextField name="TextField_LevelEndScore" enabled="True" />
 				<TextField name="TextField_LevelEndScoreEffects" enabled="True" />
				
				<TextField name="TextField_NewHighScore" enabled="True" visible="false" />

				<TextField name="TextField_BestScore" enabled="True" visible="false" />
				<MovieClip name="MovieClip_BestScoreStars" enabled="True" visible="false" />
				<MovieClip name="MovieClip_ResultMEFeather" enabled="True" visible="true" />

				
				<MovieClip name="MovieClip_NewHighScoreBadge" visible="false"/>
			</Container>
			
			<Button name="Button_Watch_Replay"  MouseUp="WATCH_REPLAY" alignH="RIGHT" scaleOnMouseOver="TRUE"/>
			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_TournamentBrandedLevelEnd>
		<!--  NEW LEVEL END VIEW    -->	
		
		<View_TournamentBrandedLevelEndFail name="TournamentBrandedViewLevelEndFail" isOverlay="True" overlayColor="0xFF0000" overlayAlpha="0" width="1024" height="768">      <!--  name parameter must be identical(case sensitive) with export name of the main movie clip of this UI view      -->
			<Container name="Container_LevelEndStripe" alignH="CENTER" alignV="CENTER">
				<MovieClip name="MovieClip_LevelEndStripeFailBG" scaleV="True" alignV="CENTER"/>/>
				
				<Button name="Button_MightyEagle"  visible="false" MouseUp="SHOP" scaleOnMouseOver="FALSE"/>
				
				<Button name="Button_Menu"  MouseUp="MENU" scaleOnMouseOver="TRUE"/>
				<Button name="Button_Replay"  MouseUp="REPLAY" scaleOnMouseOver="TRUE"/>
				<Button name="Button_NextLevel"  MouseUp="NEXT_LEVEL" scaleOnMouseOver="TRUE"/>
				<Button name="Button_CutScene"  MouseUp="NEXT_LEVEL" visible="false" scaleOnMouseOver="TRUE"/>
				
				<Button name="Button_FreePowerups"  MouseUp="FREE_POWERUPS" scaleOnMouseOver="TRUE" visible="True"/>
				
				<Button name="Button_Share_Default"  MouseUp="SHARE_DEFAULT" scaleOnMouseOver="TRUE" visible="True"/>
				<Button name="Button_Embed"  MouseUp="EMBED" scaleOnMouseOver="TRUE" visible="True"/>
				
				<MovieClip name="MovieClip_LevelEndFailedPig" visible="true"/>
				
			</Container>
			
			<Button name="Button_FullScreen" MouseUp="FULLSCREEN_BUTTON"  alignH="RIGHT" alignV="BOTTOM" scaleOnMouseOver="TRUE"/>
		</View_TournamentBrandedLevelEndFail>
		
		<View_BrandedTournament name="TournamentLevelSelectionBranded" width="1188" height="571">
			<MovieClip name="loadingTournament" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP" alignH="CENTER" alignV="CENTER"/>
			
			
			<Container name="CombinedBackground" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP" alignH="CENTER" alignV="CENTER">
			</Container>
			
			<Container name="TextContainer" alignH="CENTER" alignV="TOP" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP">
				<TextField name="DaysLeftTextfield"/>
				<TextField name="TournamentTitleTextField"/>
			</Container>
			
			<Container name="LevelButtonsContainer" alignH="CENTER" alignV="CENTER" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="TRUE">
				<Button name="LevelButton1" MouseUp="LEVEL_1" scaleOnMouseOver="TRUE"/>
				<Button name="LevelButton2" MouseUp="LEVEL_2" scaleOnMouseOver="TRUE"/>
				<Button name="LevelButton3" MouseUp="LEVEL_3" scaleOnMouseOver="TRUE"/>
				<Button name="LevelButton4" MouseUp="LEVEL_4" scaleOnMouseOver="TRUE"/>
				<Button name="LevelButton5" MouseUp="LEVEL_5" scaleOnMouseOver="TRUE"/>
				<Button name="LevelButton6" MouseUp="LEVEL_6" scaleOnMouseOver="TRUE"/>
			</Container>
			
			<Container name="PillarContainer" alignH="CENTER" alignV="BOTTOM" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP">
				<MovieClip name="StarPillarPlaceHolder1"/>
				<MovieClip name="StarPillarPlaceHolder2"/>
				<MovieClip name="StarPillarPlaceHolder3"/>
			</Container>
			
			<!-- don't show cutscene's in valentine's -->
			<!-- <Container name="CutsceneButtonsContainer" alignH="CENTER" alignV="BOTTOM" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="true" visible="false">
				<Button name="CutsceneButton1" MouseUp="CUTSCENE_1" scaleOnMouseOver="true"/>
				<Button name="CutsceneButton2" MouseUp="CUTSCENE_2" scaleOnMouseOver="true"/>
				<Button name="CutsceneButton3" MouseUp="CUTSCENE_3" scaleOnMouseOver="true"/>
				<Button name="CutsceneButton4" MouseUp="CUTSCENE_4" scaleOnMouseOver="true"/>
				<Button name="CutsceneButton5" MouseUp="CUTSCENE_5" scaleOnMouseOver="true"/>
			</Container>
			 -->
			<!-- <Button name="Button_BrandedShopButton" MouseUp="BRANDED_SHOP" MouseOver="BRANDED_SHOP_OVER" MouseOut="BRANDED_SHOP_OUT" alignH="LEFT" alignV="TOP" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP" visible="false"/>
			<MovieClip name="Button_BrandedShopButtonAnimation" alignH="LEFT" alignV="TOP" scaleH="TRUE" scaleV="TRUE" aspectRatioFixed="UP" visible="false"/> -->
			
			<Button name="Button_Back" MouseUp="BACK" alignH="LEFT" alignV="BOTTOM"/>
			<Button name="Button_PreviousResults" MouseUp="PREVIOUS_RESULTS" alignH="RIGHT" alignV="BOTTOM"/>
		</View_BrandedTournament>
		
		
		
		<!-- BRANDED END TOURNAMENT STUFF -->
		
		
	</Views>
	<Components>
		
	</Components>
</UIDescription>